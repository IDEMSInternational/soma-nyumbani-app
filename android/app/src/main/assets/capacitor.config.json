{
  "appId": "international.idems.somanyumbani",
  "appName": "Soma Nyumbani",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": false
    },
    "CapacitorFirebaseAuth": {
      "providers": ["google.com"],
      "languageCode": "en",
      "nativeAuth": true,
      "permissions": {
        "google": ["profile"]
      }
    }
  },
  "cordova": {}
}
